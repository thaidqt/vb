<script type="text/javascript">
	const versionSw = '1.3.9';
	(async () => {
		if ('serviceWorker' in navigator) {
			navigator.serviceWorker.register(`/service_worker.js?v=${versionSw}`).then(swReg => {
				if (swReg && swReg.active && swReg.active.scriptURL && !swReg.active.scriptURL.includes(versionSw)) swReg.update();
			}).catch(err => {
				console.error('SW register ERROR', err);
			});
		}
	})()
</script>
