    <script>
		function getCookie(n){for(var t=n+"=",e=document.cookie.split(";"),r=0;r<e.length;r++){for(var i=e[r];" "==i.charAt(0);)i=i.substring(1,i.length);if(0==i.indexOf(t))return i.substring(t.length,i.length)}return null}
		function setCookie(e,i,t,o,n){var a=e+"="+i;if(t){var r=new Date;r.setTime(r.getTime()+24*t*60*60*1e3),a+="; expires="+r.toGMTString()}a+="; path=/",o&&(a+="; domain="+o),n&&(a+="; secure"),document.cookie=a}
		function delCookie(key){document.cookie = key + '=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;';}
		function sel(elm, is_multi = false, parent = document) {
			if (is_multi) {
				return parent.querySelectorAll(elm);
			} else {
				return parent.querySelector(elm);
			}
		}

		const urlParams = new URLSearchParams(window.location.search);
		const DATA_API  = 'https://api.thapcam.xyz/api';
		//const DATA_API  = 'http://localhost:91/api';
		const AUTH_URL  = 'https://auth2.vebotv.me';
		const TOKEN_KEY = 'skSuQhWnq7RZYrv';
		const ROOM_ID   = '' || '' || 'all';

		function getToken() {
			return localStorage.getItem(TOKEN_KEY) || getCookie(TOKEN_KEY);
		}
		function saveToken(access_token) {
			localStorage.setItem(TOKEN_KEY, access_token);
			setCookie(TOKEN_KEY, access_token, 365);
		}
		function removeToken() {
			delCookie(TOKEN_KEY);
			localStorage.removeItem(TOKEN_KEY);
		}
		function loginUrl() {
			return `${AUTH_URL}/check-login?callback=${encodeURIComponent(window.location.origin + window.location.pathname)}`;
        }
		function renderUser(user = null) {
			if (user) {
				$('.user-display').html(`<div class="dropdown for-member">
                  <button type="button" id="for-member" class="btn-avatar" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <img class="logged-avatar" src="${user.avatar_url}" alt="${user.name}" title="${user.name}">
                  </button>
                  <div class="dropdown-menu">
                    <a href="${AUTH_URL}/tai-khoan" class="dropdown-item" target="_blank" rel="noreferrer nofollow" >
                        <i class="fas fa-user mr-2"></i> Tài khoản
                    </a>
                    <a class="dropdown-item di-bottom btn_logout" rel="noreferrer nofollow" href="javascript:void(0)">
                      <span class="text-danger"><i class="fas fa-sign-out-alt mr-2"></i> Đăng xuất</span>
                    </a>
                  </div>
                </div>`);
			}
			else {
				$('.user-display').html(`<div class="dropdown for-member">
                  <button type="button" id="for-guest" class="btn-avatar" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" aria-label="User">
                    <svg xmlns="http://www.w3.org/2000/svg" width="40" height="40" viewBox="0 0 36 36" fill="none">
                        <path d="M18.2038 3.38295C9.97864 3.27186 3.2638 9.9867 3.37489 18.2119C3.48458 26.1002 9.90763 32.5233 17.796 32.6329C26.0226 32.7455 32.736 26.0306 32.6235 17.8055C32.5152 9.91569 26.0922 3.49264 18.2038 3.38295ZM27.0927 26.3927C27.0647 26.423 27.0304 26.4468 26.9922 26.4623C26.9539 26.4778 26.9128 26.4848 26.8716 26.4826C26.8304 26.4805 26.7902 26.4693 26.7538 26.4499C26.7174 26.4304 26.6857 26.4033 26.661 26.3702C26.0321 25.5474 25.2619 24.8429 24.3864 24.2897C22.5962 23.1408 20.3279 22.5079 17.9999 22.5079C15.6718 22.5079 13.4036 23.1408 11.6134 24.2897C10.7379 24.8427 9.96774 25.5469 9.3388 26.3695C9.31409 26.4025 9.28242 26.4297 9.24603 26.4492C9.20963 26.4686 9.16941 26.4798 9.12821 26.4819C9.08702 26.4841 9.04585 26.4771 9.00763 26.4616C8.96942 26.4461 8.93509 26.4223 8.90708 26.392C6.84388 24.1648 5.67516 21.2545 5.62489 18.2189C5.51028 11.3768 11.1247 5.64983 17.9697 5.63295C24.8146 5.61608 30.3749 11.1743 30.3749 18.008C30.3773 21.1168 29.2049 24.1117 27.0927 26.3927Z" fill="#67696F"></path>
                        <path d="M17.9999 10.1289C16.6134 10.1289 15.3597 10.6485 14.4688 11.5928C13.578 12.5371 13.1329 13.8428 13.2335 15.2441C13.4374 18.0039 15.5756 20.2539 17.9999 20.2539C20.4243 20.2539 22.5583 18.0039 22.7664 15.2448C22.8705 13.8569 22.4289 12.5631 21.5233 11.6012C20.6289 10.652 19.3774 10.1289 17.9999 10.1289Z" fill="#67696F"></path>
                    </svg>
                  </button>
                  <div class="dropdown-menu user-login">
                      <div class="text">
                          <div class="t">Chào huynh đài,</div>
                          <div>Huynh đài cần đăng nhập để tham gia bình luận và sử dụng các tính năng nâng cao nhé.</div>
                      </div>
                      <a class="google-btn" href="${loginUrl()}" rel="noreferrer nofollow">
                          <div>
                              <img class="google-icon" src="/images/g.svg"/>
                          </div>
                          <p class="btn-text">Sign in with Google</p>
                      </a>
                  </div>
                </div>`);
			}
		}
		function renderChatbox(chat_urls = null, user = null, chat_token = null) {
			let btn_show_chat = $(".btn-tchat");
			let show_chat = false;
			if(btn_show_chat && btn_show_chat.length && !btn_show_chat.hasClass("slash")){
				show_chat = true;
            }
			$('.google-btn').attr('href', loginUrl());
			let chat_all  = 'https://www6.cbox.ws/box/?boxid=827198&amp;boxtag=wskql9&amp;tid=2&amp;tkey=caec662bbe2b279a';
			let chat_room = 'https://chat.vebotv.me/g';

			if (chat_urls && chat_urls.default) {
				chat_all = chat_urls.default || chat_all;
			}
			if (!user) {
				$('.chat-require-login').addClass('active');
			}
			else {
				$('.chat-require-login').removeClass('active');
			}
			let tk = encodeURIComponent(chat_token || '');
			tk = `${chat_room}?room=${ROOM_ID}&token=${tk}`;
			$('#chat-all').html(`<iframe class="cbf" src="${show_chat ? chat_all : ''}" data-src="${chat_all}" width="100%" height="100%" allowtransparency="yes" allow="autoplay" frameborder="0" marginheight="0" marginwidth="0" scrolling="auto"></iframe>`);
			$('#chat-room').html(`<iframe src="${show_chat ? tk : ''}" data-src="${tk}" width="100%" height="100%" loading="eager" allowtransparency="yes" allow="autoplay" frameborder="0" marginheight="0" marginwidth="0" allowfullscreen></iframe>`);
		}
		async function getData(url, method = 'POST', data = {}) {
			let z       = {
				url        : url,
				type       : method,
				xhrFields  : {withCredentials: true},
				crossDomain: true,
			};
			const token = getToken();
			if (token) {
				z.beforeSend = function (request) {
					request.setRequestHeader("Authorization", `Bearer ${token}`);
				}
			}
			if (method === 'POST') {
				z.data     = data;
				z.dataType = 'json';
				z.encode   = true;
			}
			return $.ajax(z);
		}
		async function fetchUserFromToken() {
			let c = await getData(`${AUTH_URL}/v1/user/profile`, 'GET').then((data) => {
				let status       = data.status || 0;
				let user         = data.user || null;
				let access_token = data.token || null;
				let chat_urls    = data.chat_urls || null;
				let chat_token   = data.new_chat_token || null;
				renderChatbox(chat_urls, user, chat_token);
				if ([1, -7].includes(status)) {
					if (access_token) {
						saveToken(access_token);
					}
					return {
						user,
						access_token,
					};
				}
				return {
					user        : null,
					access_token: null,
				};
			});
			return c;
		}
		async function fetchUserFromCookie() {
			let c = await getData(`${AUTH_URL}/v1/user/profile-cookie`, 'GET').then((data) => {
				let status       = data.status || 0;
				let user         = data.user || null;
				let access_token = data.token || null;
				let chat_urls    = data.chat_urls || null;
				let chat_token   = data.new_chat_token || null;
				renderChatbox(chat_urls, user, chat_token);
				if ([1, -7].includes(status)) {
					if (access_token) {
						saveToken(access_token);
					}
					return {
						user,
						access_token,
					};
				}
				return {
					user        : null,
					access_token: null,
				};
			});
			return c;
		}
		async function fetchUser() {
			const token = getToken();
			if (token) {
				return fetchUserFromToken();
			} else {
				return fetchUserFromCookie();
			}
		}
		async function logout() {
			removeToken();
			await getData(`${AUTH_URL}/v1/auth/logout`, 'GET').then((data) => {
				renderChatbox(data.chat_urls || null, null, null);
			});
		}
		/*function isv(str) {
			const v6Regex = /^([0-9a-fA-F]{1,4}:){7}[0-9a-fA-F]{1,4}$/;
			return v6Regex.test(str);
		}
		async function pi() {
			let pi = '';
			await fetch(atob('aHR0cHM6Ly9jaGVja2lwLmZwdHBsYXkubmV0Lw==')).then(response => response.text()).then(data => {
				pi = data || '';
			});

			if (!pi || isv(pi)) {
				await fetch(atob('aHR0cHM6Ly9hcGkuaXBpZnkub3JnP2Zvcm1hdD1qc29u')).then(response => response.json()).then(data => {
					pi = data.ip || '';
				});
			}
			if (!pi || isv(pi)) {
				await fetch(atob('aHR0cDovL2lwLWFwaS5jb20vanNvbi8=')).then(response => response.json()).then(data => {
					pi = data.query || '';
				});
			}
			if (!pi || isv(pi)) {
				return 'no';
			}
			return btoa(encodeURIComponent(pi)).replace(/=/g, '');
		}*/
		const tk = urlParams.get('_vb');
		if (tk) {
			saveToken(tk);
			fetchUser();
			window.history.pushState({}, document.title, window.location.origin + window.location.pathname);
		}
